<%content_for :breadcrumb do%>
  <ul class="breadcrumb">
    <%if signed_in?%>
    <li><%=link_to @current_user.branch.name, @current_user.branch%><span class="divider">/</span></li>
    <%else%>
        <li><%=link_to t(:home), root_path%><span class="divider">/</span></li>
    <%end%>
    <%if can? :index, Client%>
      <li><%=link_to t(:index), clients_path%><span class="divider">/</span></li>
    <%end%>
    <li class="active"><%=t(:new) + " " + t(:wclients)%></li>
  </ul>
<%end%>
<%= render 'form' %>
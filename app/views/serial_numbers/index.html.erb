<h1>Listing serial_numbers</h1>

<table>
  <tr>
    <th>Part</th>
    <th>Serial number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @serial_numbers.each do |serial_number| %>
  <tr>
    <td><%= serial_number.part %></td>
    <td><%= serial_number.serial_number %></td>
    <td><%= link_to 'Show', serial_number %></td>
    <td><%= link_to 'Edit', edit_serial_number_path(serial_number) %></td>
    <td><%= link_to 'Destroy', serial_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Serial number', new_serial_number_path %>
